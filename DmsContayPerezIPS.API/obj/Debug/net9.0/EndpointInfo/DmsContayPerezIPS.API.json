                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  },
                  "tipoDocId": {
                    "type": "integer",
                    "format": "int64"
                  },
                  "documentDate": {
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                },
                "tipoDocId": {
                  "style": "form"
                },
                "documentDate": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
